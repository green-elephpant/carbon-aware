{
  "name": "green-elephpant/carbon-aware",
  "description": "SDK to help your applications reduce carbon emissions",
  "version": "0.0.2",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Carsten Windler",
      "email": "carsten@carstenwindler.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "GreenElephpant\\CarbonAware\\": "src/",
      "GreenElephpant\\CarbonAware\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.4",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/simple-cache": "^1.0 || ^3.0",
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.3",
    "nyholm/psr7": "^1.8"
  },
  "minimum-stability": "stable"
}
